{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "json.schemas": [
    {
      "fileMatch": ["package.json"],
      "url": "./.vscode/schemas/package.schema.json"
    }
  ],
  "json.schemaDownload.enable": false
}
